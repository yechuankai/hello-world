	<queryString>
		<![CDATA[SELECT 
       T.Application_No,
       NVL(T.IC_CARD_COMPANY_ID,C.SHORTNAME) COMPANYNAME,
       T.APPLICANT_NAME Name,
       $P{logicCardNo} Card_NO,
       T.POSTION,
       ' ' Remark,
       (select MAX(SYS_URL) from uums.Web_Sys_Tfp WHERE SYS_CODE = 'DIFTZ_PS') 
       || '/common/attachedFile/previewFile.action?objectId=' 
       || F.OBJECT_ID
       || '&fileName='
       || $P{logicCardNo} IMG
  FROM 
       DPSP.CARD_INFO_REGULAR T
       INNER JOIN UUMS.COMPANY C ON C.COMPANY_ID = T.COMPANY_ID
  LEFT JOIN 
       DPSP.COMMON_ATTACHED_FILE F ON T.Application_No = F.APPLICATION_NO AND F.REMARK = '1' AND F.DELETE_FLAG = 'N'
  WHERE 
       T.Application_No = $P{applicationNo}
  ORDER BY 
        T.Application_No]]>
