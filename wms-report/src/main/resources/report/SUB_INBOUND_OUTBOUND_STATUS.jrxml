	<queryString>
		<![CDATA[with inbound as
 (select $P{web.label.inbound} type,
         decode(status, '40', 'Close', '20', 'Processing', 'New') status
    from inbound_header_t
   where status in ('10', '20', '30', '40')
     and (company_id = $P{companyId} or nvl($P{companyId},0) = 0)
     and del_flag = 'N'),
outbound as
 (select $P{web.label.outbound} type,
         case
           when status in ('70', '80') then
            'Close'
           when status in ('20', '30', '40', '50', '60') then
            'Processing'
           else
            'New'
         end status
    from outbound_header_t
   where status in ('10', '20', '30', '40', '50', '60', '70', '80')
     and (company_id = $P{companyId} or nvl($P{companyId},0) = 0)
     and del_flag = 'N')
select type,
       sum(decode(status, 'New', 1, 0)) New,
       sum(decode(status, 'Processing', 1, 0)) Processing,
       sum(decode(status, 'Close', 1, 0)) Close
  from (select * from inbound union all select * from outbound) t
 group by type]]>
	</queryString>
