	<queryString>
		<![CDATA[SELECT 
       LABOR.Application_No,
       LABOR.Seq_No,
       LABOR.LABOR_NAME Labor_Name,
       $P{logicCardNo} Card_NO,
       LABOR.LABOR_ID_NO Photo_Name,
       ' ' Remark,
       (select MAX(SYS_URL) from uums.Web_Sys_Tfp WHERE SYS_CODE = 'DIFTZ_PS') 
       || '/common/attachedFile/previewFile.action?objectId=' 
       || F.OBJECT_ID
       || '&fileName='
       || $P{logicCardNo} IMG
  FROM 
       DPSP.CARD_INFO_LABOR_LIST LABOR
  LEFT JOIN 
       DPSP.COMMON_ATTACHED_FILE F ON LABOR.SEQ_NO = F.APPLICATION_NO AND F.REMARK = '1'
  WHERE 
       LABOR.SEQ_NO = $P{applicationNo}
  ORDER BY 
        LABOR.SEQ_NO]]>
	</queryString>
